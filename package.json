{
  "name": "vue3-rolling-number-input",
  "version": "0.5.5",
  "description": "A Vue 3 number input component that allows using the mouse wheel to adjust individual digits.",
  "author": "P. J. Reed <phillipreed@hatchbed.com>",
  "homepage": "https://github.com/hatchbed/vue-rolling-number-input",
  "bugs": "https://github.com/hatchbed/vue-rolling-number-input/issues",
  "license": "BSD-3-Clause",
  "type": "module",
  "files": [
    "dist"
  ],
  "typings": "./dist/vue3-rolling-number-input.d.ts",
  "main": "./dist/vue3-rolling-number-input.umd.cjs",
  "module": "./dist/vue3-rolling-number-input.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-rolling-number-input.js",
      "require": "./dist/vue3-rolling-number-input.umd.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "keywords": [
    "vue",
    "vuejs",
    "number",
    "numeric",
    "input",
    "spinner",
    "number input",
    "number spinner",
    "numeric input",
    "numeric spinner",
    "mouse",
    "mousewheel",
    "wheel"
  ],
  "dependencies": {
    "core-js": "^3.30.2",
    "vue": "^3.3.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "eslint": "^8.40.0",
    "eslint-plugin-vue": "^9.12.0",
    "typescript": "^5.0.4",
    "vite": "^4.2.2",
    "vite-plugin-dts": "^2.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
